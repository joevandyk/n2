-# @expects: local 'newest_classifieds'
- cache :newest_classifieds_html do
  - newest_classifieds ||= Classified.active.available.allow_all.newest 5
  - unless newest_classifieds.empty?
    = render :partial => 'shared/lists/small_generic_list', :locals => { :list_title => t('classifieds.newest_classifieds_title'), :item_list => newest_classifieds }